{
  "name": "r-thedragonprince",
  "description": "Subreddit theme for the Netflix show \"The Dragon Prince\"",
  "version": "1.0.5",
  "main": "src/sass/theme.scss",
  "scripts": {
    "dev": "webpack-dev-server --env.NODE_ENV=development",
    "prod": "webpack --env.NODE_ENV=production",
    "prod-test": "webpack-dev-server --env.NODE_ENV=production --env.test=true",
    "prod-test-light": "webpack-dev-server --env.NODE_ENV=production --env.test=true --env.light=true",
    "test": "npm run prod && bundlesize"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowzy/r-thedragonprince.git"
  },
  "keywords": [
    "sass",
    "webpack",
    "webpack4",
    "reddit",
    "theme",
    "reddit-theme",
    "thedragonprince"
  ],
  "author": "flowzy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flowzy/r-thedragonprince/issues"
  },
  "homepage": "https://github.com/flowzy/r-thedragonprince#readme",
  "browserslist": [
    "last 1 major version",
    "not dead",
    "> 1%"
  ],
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "100 kB",
      "compression": "none"
    },
    {
      "path": "./dist/img/*/*.jpg",
      "maxSize": "500 kB",
      "compression": "none"
    },
    {
      "path": "./dist/img/*.png",
      "maxSize": "500 kB",
      "compression": "none"
    },
    {
      "path": "./dist/img/*/*.png",
      "maxSize": "500 kB",
      "compression": "none"
    }
  ],
  "dependencies": {
    "autoprefixer": "^9.4.6",
    "bundlesize": "^0.17.1",
    "clean-terminal-webpack-plugin": "^2.0.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-merge-rules": "^4.0.2",
    "replace-css-url-loader": "^1.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-fix-style-only-entries": "^0.1.0",
    "webpack-spritesmith": "^1.0.0"
  }
}
